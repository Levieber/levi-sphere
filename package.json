		"rxjs": "7.8.1"
	},
	"devDependencies": {
		"@commitlint/cli": "19.6.0",
		"@commitlint/config-conventional": "19.6.0",
		"@eslint/eslintrc": "3.2.0",
		"@eslint/js": "9.15.0",
		"@nestjs/cli": "10.4.8",
		"@nestjs/schematics": "10.2.3",
		"@nestjs/testing": "10.4.8",
		"@swc/cli": "0.5.1",
		"@swc/core": "1.9.3",
		"@types/express": "5.0.0",
		"@types/jest": "29.5.14",
		"@types/node": "20.17.7",
		"@types/supertest": "6.0.2",
		"@typescript-eslint/eslint-plugin": "8.15.0",
		"@typescript-eslint/parser": "8.15.0",
		"eslint": "9.15.0",
		"eslint-config-prettier": "9.1.0",
		"globals": "15.12.0",
		"husky": "9.1.7",
		"jest": "29.7.0",
		"lint-staged": "15.2.10",
		"prettier": "3.3.3",
		"source-map-support": "0.5.21",
		"supertest": "7.0.0",
		"ts-jest": "29.2.5",
		"ts-loader": "9.5.1",
		"ts-node": "10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.6.3"
	}
